local function SFYYEDD_fake_script() -- MoonWareLock.LocalScript 
	local script = Instance.new('LocalScript', MoonWareLock)

	script.Parent.MouseButton1Up:Connect(function()
		setclipboard("https://www.roblox.com/game-pass/35031212/MoonLight")
		script.Parent.Parent.TextLabel.Visible = true
		wait(5)
		script.Parent.Parent.TextLabel.Visible = false
		wait(0.7)
		script.Parent.Parent.Parent.InfoFrame.Visible = t
	end)
end
coroutine.wrap(SFYYEDD_fake_script)()
local function ORRO_fake_script() -- SpeedC.LocalScript 
	local script = Instance.new('LocalScript', SpeedC)

	script.Parent.MouseButton1Up:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/angeloX041/Speedc/main/SuperSpeedC ", true))()
	end)
end
coroutine.wrap(ORRO_fake_script)()
