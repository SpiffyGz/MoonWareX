	local script = Instance.new('LocalScript', SettingsFrame)

	script.Parent.MouseButton1Up:Connect(function()
		print("Settings Frame")
		script.Parent.Parent.Parent.HomeFrame.Visible = false
		script.Parent.Parent.Parent.SettingFrame.Visible = true
		script.Parent.Parent.Parent.InfoFrame.Visible = false
	end)
end
