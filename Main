local function EDLP_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local MainFrame = script.Parent.MainFrame
	local Open = false
	
	local PositionClosed = UDim2.new(0.346, 0,0.999, 0)
	local PositionOpen = UDim2.new(0.306, 0,0.144, 0)
	
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(KeyCode)
		if KeyCode.KeyCode == Enum.KeyCode.RightControl then
			if Open then
				MainFrame:TweenPosition((PositionClosed), "Out", "Sine")
				Open = false
			else
				Open = true
				MainFrame:TweenPosition((PositionOpen), "Out", "Sine")
			end
		end
	end)
	
end
coroutine.wrap(EDLP_fake_script)()
