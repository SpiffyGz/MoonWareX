-- Scripts:

local function NEPYV_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local KeyFrame = script.Parent.KeyFrame
	local MainFrame = script.Parent.MainFrame
	local LoadScript = script.Parent.KeyFrame.LoadScript
	local KeyHere = script.Parent.KeyFrame.KeyHere
	
	
	LoadScript.MouseButton1Down:Connect(function()
		if KeyHere.Text == "MoonWareX" then
			KeyFrame:TweenPosition(UDim2.new(0.417, 0.417 ,0 , 1080))
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "MoonWareX";
				Text = "Loading";
				Duration = 9e9;
			})
	
			wait(2)
			KeyFrame.Visible = false
			wait(1)
			MainFrame.Visible = true
	
	
		end
	end)
end
coroutine.wrap(NEPYV_fake_script)()
local function ENJGNE_fake_script() -- HomeButton.LocalScript 
	local script = Instance.new('LocalScript', HomeButton)

	script.Parent.MouseButton1Up:Connect(function()
		print("Home Frame")
		script.Parent.Parent.Parent.HomeFrame.Visible = true
		script.Parent.Parent.Parent.SettingFrame.Visible = false
		script.Parent.Parent.Parent.InfoFrame.Visible = false
	end)
end
coroutine.wrap(ENJGNE_fake_script)()
